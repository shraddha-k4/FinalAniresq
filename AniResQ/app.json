{
  "expo": {
    "name": "AniResQ",
    "slug": "aniresq",
    "scheme": "aniresq",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/aniresq.png",
    "userInterfaceStyle": "light",
   
    "ios": {
      "supportsTablet": true
    },
    "android": {
     

 "adaptiveIcon": {
  "foregroundImage": "./assets/aniresq.png"
  // "backgroundColor": "transparent"
},
      "package": "com.anonymous.aniresq"
    },
    "web": {
      "favicon": "./assets/img/favicon.png"
    },
    "plugins": [
      "expo-router",
      "@react-native-community/datetimepicker"
    ]
  }
}

